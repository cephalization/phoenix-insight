{
  "name": "phoenix-insight-monorepo",
  "private": true,
  "description": "Monorepo for Phoenix Insight CLI and UI packages",
  "type": "module",
  "scripts": {
    "clean": "pnpm -r run clean",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:ui": "pnpm build && vitest run --config vitest.config.ts",
    "typecheck": "pnpm -r run typecheck",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "author": "Tony Powell <powell.anthonyd@proton.me>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/cephalization/phoenix-insight#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cephalization/phoenix-insight.git"
  },
  "bugs": {
    "url": "https://github.com/cephalization/phoenix-insight/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^18.19.0",
    "agent-browser": "^0.5.0",
    "rimraf": "^5.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  }
}
